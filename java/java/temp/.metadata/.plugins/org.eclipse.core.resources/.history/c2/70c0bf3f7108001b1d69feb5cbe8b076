package test;
/*
 * private,隐藏实现的细节，对外提供接口和方法 
 * 
 */
class Pri{
	String name;
	private int age;
	
	public void setAge(int a) {
		age = a;
		System.out.println(age);
	}
	
	private void method() {
		System.out.println("private");
	}
}

public class fengzhuang0704 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Pri p = new Pri();
		p.setAge(1);
		
	}

}
