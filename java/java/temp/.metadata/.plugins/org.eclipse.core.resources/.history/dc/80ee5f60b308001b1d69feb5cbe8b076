package test;
/*
 * 代码块
 *  局部代码块，构造代码块，静态代码快
 *  
 *  构造代码块：构造函数共有的块
 *  静态代码块：static修饰
 */






public class a0808_code {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = {1,2,3,4,4};
//		printArr(arr);
		
		Arraytool al = new Arraytool();
		al.printArr(arr);
		
		System.out.println("--------------------");
		
		int ran = (int)(Math.random()*100)+1;
		System.out.println(ran);
		
	}
	
	
	

}






